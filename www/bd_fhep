--
-- Índices para tablas volcadas
--

--
-- Indices de la tabla `estado`
--
ALTER TABLE `estado`
  ADD PRIMARY KEY (`id`);

--
-- Indices de la tabla `tbl_afiliacion`
--
ALTER TABLE `tbl_afiliacion`
  ADD PRIMARY KEY (`id`),
  ADD KEY `FK cod_status` (`id_status`) USING BTREE,
  ADD KEY `FK cod_persona` (`id_persona`) USING BTREE,
  ADD KEY `tbl_ afiliacion_ibfk_5` (`id_tipafiliado`);

--
-- Indices de la tabla `tbl_categoria`
--
ALTER TABLE `tbl_categoria`
  ADD PRIMARY KEY (`id`);

--
-- Indices de la tabla `tbl_cit_medicas`
--
ALTER TABLE `tbl_cit_medicas`
  ADD PRIMARY KEY (`id`),
  ADD KEY `FK cod_estado_cita` (`id_estado_cita`) USING BTREE,
  ADD KEY `FK cod_tipo_cita` (`id_tipo_cita`) USING BTREE,
  ADD KEY `FK cod_medico` (`id_medico`) USING BTREE,
  ADD KEY `FK cod_especialidad` (`id_especialidad`) USING BTREE,
  ADD KEY `FK cod_horario` (`id_horario`) USING BTREE,
  ADD KEY `FK cod_afiliado` (`id_afiliado`) USING BTREE;

--
-- Indices de la tabla `tbl_comentarios_medicos`
--
ALTER TABLE `tbl_comentarios_medicos`
  ADD KEY `FK cod_medico` (`id_medico`) USING BTREE,
  ADD KEY `FK Id_expediente_clinico` (`id`) USING BTREE;

--
-- Indices de la tabla `tbl_datos_policiales`
--
ALTER TABLE `tbl_datos_policiales`
  ADD PRIMARY KEY (`Id`),
  ADD KEY `id_persona` (`id_persona`),
  ADD KEY `n_placa_policial` (`n_placa_policial`),
  ADD KEY `id_cod_dependencia` (`id_cod_dependencia`),
  ADD KEY `id_rango` (`id_rango`),
  ADD KEY `id_categoria` (`id_categoria`),
  ADD KEY `id_departamento` (`id_departamento`);

