  ADD CONSTRAINT `tbl_medico_ibfk_1` FOREIGN KEY (`id_persona`) REFERENCES `tbl_personas` (`id`),
  ADD CONSTRAINT `tbl_medico_ibfk_2` FOREIGN KEY (`id_especialidad`) REFERENCES `tbl_especialidades` (`id`);

--
-- Filtros para la tabla `tbl_ms_bitacora`
--
