CREATE TABLE `tbl_ms_preguntas` (
  `id` int(20) NOT NULL,
  `pregunta` varchar(100) NOT NULL,
  `estado_p` int(1) NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

--
-- Volcado de datos para la tabla `tbl_ms_preguntas`
--

