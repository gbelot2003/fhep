(5, 'ffsadas', 0),
(6, 'sadasd', 0),
(7, 'SDASD', 0);

--
-- Disparadores `tbl_ms_preguntas`
--
DELIMITER $$
CREATE TRIGGER `Inser pegruntas` BEFORE INSERT ON `tbl_ms_preguntas` FOR EACH ROW INSERT into bitacora   (Fecha,	id_usuario,id_objeto,Accion,Descripcion) VALUES (NOW(),CURRENT_USER,'1','insert','Descripcion')
$$
DELIMITER ;

