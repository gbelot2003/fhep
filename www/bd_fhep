  ADD CONSTRAINT `tbl_comentarios_medicos_ibfk_1` FOREIGN KEY (`id`) REFERENCES `tbl_exp_clinico` (`id`);

--
-- Filtros para la tabla `tbl_datos_policiales`
--
ALTER TABLE `tbl_datos_policiales`
  ADD CONSTRAINT `tbl_datos_policiales_ibfk_1` FOREIGN KEY (`id_persona`) REFERENCES `tbl_personas` (`id`),
  ADD CONSTRAINT `tbl_datos_policiales_ibfk_2` FOREIGN KEY (`n_placa_policial`) REFERENCES `tbl_placa_policial` (`Id`),
  ADD CONSTRAINT `tbl_datos_policiales_ibfk_3` FOREIGN KEY (`id_cod_dependencia`) REFERENCES `tbl_dependencias` (`id`),
  ADD CONSTRAINT `tbl_datos_policiales_ibfk_4` FOREIGN KEY (`id_rango`) REFERENCES `tbl_rangos` (`id`),
  ADD CONSTRAINT `tbl_datos_policiales_ibfk_5` FOREIGN KEY (`id_categoria`) REFERENCES `tbl_categoria` (`id`),
  ADD CONSTRAINT `tbl_datos_policiales_ibfk_6` FOREIGN KEY (`id_departamento`) REFERENCES `tbl_departamentos` (`id`);

