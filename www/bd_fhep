  ADD CONSTRAINT `tbl_exp_clinico_ibfk_1` FOREIGN KEY (`id_afiliado`) REFERENCES `tbl_afiliacion` (`id`),
  ADD CONSTRAINT `tbl_exp_clinico_ibfk_2` FOREIGN KEY (`id_medico`) REFERENCES `tbl_medico` (`id`),
  ADD CONSTRAINT `tbl_exp_clinico_ibfk_3` FOREIGN KEY (`id_cita`) REFERENCES `tbl_cit_medicas` (`id`);

--
-- Filtros para la tabla `tbl_medico`
--
