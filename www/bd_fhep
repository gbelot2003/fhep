--
-- Filtros para la tabla `tbl_afiliacion`
--
ALTER TABLE `tbl_afiliacion`
  ADD CONSTRAINT `tbl_afiliacion_ibfk_1` FOREIGN KEY (`id_persona`) REFERENCES `tbl_personas` (`id`),
  ADD CONSTRAINT `tbl_afiliacion_ibfk_4` FOREIGN KEY (`id_status`) REFERENCES `tbl_sta_afiliado` (`id`),
  ADD CONSTRAINT `tbl_afiliacion_ibfk_5` FOREIGN KEY (`id_tipafiliado`) REFERENCES `tbl_tip_afiliado` (`id`),
  ADD CONSTRAINT `tbl_afiliacion_ibfk_7` FOREIGN KEY (`id_status`) REFERENCES `tbl_sta_afiliado` (`id`);

--
-- Filtros para la tabla `tbl_cit_medicas`
--
ALTER TABLE `tbl_cit_medicas`
  ADD CONSTRAINT `tbl_cit_medicas_ibfk_1` FOREIGN KEY (`id_afiliado`) REFERENCES `tbl_afiliacion` (`id`),
  ADD CONSTRAINT `tbl_cit_medicas_ibfk_2` FOREIGN KEY (`id_tipo_cita`) REFERENCES `tbl_tipo_cita` (`id`),
  ADD CONSTRAINT `tbl_cit_medicas_ibfk_3` FOREIGN KEY (`id_estado_cita`) REFERENCES `tbl_sta_cit_med` (`id`);

--
-- Filtros para la tabla `tbl_comentarios_medicos`
--
